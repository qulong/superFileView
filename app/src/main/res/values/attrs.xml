<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GroupRecyclerView">
        <attr name="group_height" format="dimension" />
        <attr name="group_child_offset" format="dimension" />
        <attr name="group_background" format="color" />
        <attr name="group_text_color" format="color" />
        <attr name="group_text_size" format="dimension" />
        <attr name="group_center" format="boolean" />
        <attr name="group_padding_left" format="dimension" />
        <attr name="group_padding_right" format="dimension" />
        <attr name="group_has_header" format="boolean" />
    </declare-styleable>
<declare-styleable name="MyToolbarView">
    <attr name="toolbar_title_tv" format="string"/>
    <attr name="toolbar_title_color" format="reference|color"/>
    <attr name="toolbar_right_tv_color" format="reference|color"/>
    <attr name="toolbar_right_tv" format="string"/>
    <attr name="toolbar_right_visible" format="boolean"/>
    <attr name="toolbar_share_img" format="reference"/>
    <attr name="toolbar_back_img" format="reference"/>
    <attr name="toolbar_right_visibility" format="boolean"/>
    <attr name="toolbar_share_visible" format="boolean"/>
</declare-styleable>

    <declare-styleable name="RatingStarView">
        <attr name="rsv_starMargin" format="integer|dimension" />
        <attr name="rsv_cornerRadius" format="float|dimension" />
        <attr name="rsv_starForegroundColor" format="color|reference" />
        <attr name="rsv_starBackgroundColor" format="color|reference" />
        <attr name="rsv_strokeColor" format="color|reference" />
        <attr name="rsv_strokeWidth" format="float|dimension" />
        <attr name="rsv_starNum" format="integer" />
        <attr name="rsv_rating" format="float" />
        <attr name="rsv_starThickness" format="float|fraction" />
        <attr name="rsv_drawStrokeForEmptyStar" format="boolean" />
        <attr name="rsv_drawStrokeForFullStar" format="boolean" />
        <attr name="rsv_drawStrokeForHalfStar" format="boolean" />
        <attr name="rsv_enableSelectRating" format="boolean" />
        <attr name="rsv_onlyHalfStar" format="boolean" />
    </declare-styleable>

</resources>